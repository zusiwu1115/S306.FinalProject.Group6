<Window x:Name="MainMenu1" x:Class="FitnessClub.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitnessClub"
        mc:Ignorable="d"
        Title="Main Menu" Height="400" Width="500">
    <Grid Background="#FF9B0E2A">
        <Rectangle x:Name="recBackgroundImg" HorizontalAlignment="Left" Height="200" VerticalAlignment="Top" Width="492" Margin="0,41,0,0">
            <Rectangle.Fill>
                <ImageBrush ImageSource="Background Pic.jpg"/>
            </Rectangle.Fill>
        </Rectangle>
        <Rectangle x:Name="recWhiteSpace" HorizontalAlignment="Left" Height="40" Margin="0,0,-8,0" VerticalAlignment="Top" Width="492" Fill="White"/>
        <Rectangle x:Name="recLogoImg" HorizontalAlignment="Left" Height="40" VerticalAlignment="Top" Width="80">
            <Rectangle.Fill>
                <ImageBrush ImageSource="Logo.png"/>
            </Rectangle.Fill>
        </Rectangle>
    </Grid>
</Window>

        <Label x:Name="lblMembershipType1" Content="Membership Type" HorizontalAlignment="Left" Margin="14,75,0,0" VerticalAlignment="Top" Foreground="White" Height="27" Width="109"/>
        <Label x:Name="lblMembershipTypeResult1" Content="Family 1 Month" HorizontalAlignment="Left" Margin="135,76,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="98" BorderBrush="White" BorderThickness="0,0,0,1"/>
        <Label x:Name="lblStartDate" Content="Start Date" HorizontalAlignment="Left" Margin="14,104,0,0" VerticalAlignment="Top" Foreground="White" Height="27" Width="109"/>
        <Label x:Name="lblStartDateResult" Content="2018.3.1" HorizontalAlignment="Left" Margin="135,107,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="98" BorderThickness="0,0,0,1" BorderBrush="White"/>
        <Label x:Name="lblMembershipCost" Content="Membership Cost" HorizontalAlignment="Left" Margin="13,165,0,0" VerticalAlignment="Top" Foreground="White" Height="27" Width="109"/>
        <Label x:Name="lblMembershipCostResult" Content="$19.99" HorizontalAlignment="Left" Margin="135,167,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="98" BorderBrush="White" BorderThickness="0,0,0,1"/>
        <Label x:Name="lblSubtotal" Content="Subtotal" HorizontalAlignment="Left" Margin="14,195,0,0" VerticalAlignment="Top" Foreground="White" Height="27" Width="109"/>
        <Label x:Name="lblSubtotalResult" Content="$19.99" HorizontalAlignment="Left" Margin="135,195,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="98" BorderBrush="White" BorderThickness="0,0,0,1"/>
        <Label x:Name="lblAdditionalFeatures" Content="Additional Features" HorizontalAlignment="Left" Margin="14,224,0,0" VerticalAlignment="Top" Foreground="White" Height="27" Width="115"/>
        <Label x:Name="lblAdditionalFeaturesResult" Content="None" HorizontalAlignment="Left" Margin="135,226,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="98" BorderBrush="White" BorderThickness="0,0,0,1"/>
        <Label x:Name="lblTotal" Content="Total" HorizontalAlignment="Left" Margin="14,253,0,0" VerticalAlignment="Top" Foreground="White" Height="27" Width="109"/>
        <Label x:Name="lblTotalResult" Content="$19.99" HorizontalAlignment="Left" Margin="135,254,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="98" BorderBrush="White" BorderThickness="0,0,0,1"/>
        <Label x:Name="lblWeightLoss" Content="30" HorizontalAlignment="Left" Margin="426,302,0,0" VerticalAlignment="Top" Foreground="White" BorderBrush="White" BorderThickness="0,0,0,1"/>
        <Label x:Name="lblWeightLoss_Copy" Content="195" HorizontalAlignment="Left" Margin="426,329,0,0" VerticalAlignment="Top" Foreground="White" RenderTransformOrigin="0.435,2.038" BorderBrush="White" BorderThickness="0,0,0,1"/>
        <Label x:Name="lblPerformanceResult" Content="None" HorizontalAlignment="Left" Margin="425,246,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="49" RenderTransformOrigin="0.388,3.393" BorderThickness="0,0,0,1" BorderBrush="White"/>
        <Label x:Name="lblHealth" Content="Healthy" HorizontalAlignment="Left" Margin="425,273,0,0" VerticalAlignment="Top" Foreground="White" Height="28" Width="55" RenderTransformOrigin="0.388,3.393" BorderBrush="White" BorderThickness="0,0,0,1"/>
        <GroupBox x:Name="grpFitnessGoal" Header="Fitness Goal" HorizontalAlignment="Left" Margin="242,231,0,0" VerticalAlignment="Top" Foreground="White" Height="132" Width="242"/>

    </Grid>
</Window>
